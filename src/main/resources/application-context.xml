<beans xmlns="http://www.springframework.org/schema/beans"
	   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	   xmlns:context="http://www.springframework.org/schema/context"
	   xsi:schemaLocation="http://www.springframework.org/schema/beans
    http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
    http://www.springframework.org/schema/context
    http://www.springframework.org/schema/context/spring-context-3.0.xsd">

	<context:property-placeholder location="classpath:auth.properties" ignore-unresolvable="true"/>
	<context:annotation-config/>
	<context:spring-configured/>


	<context:component-scan base-package="com.hackaton"/>

	<bean id="geneOAuth" class="com.roche.google.oauth2.SpringOAuth2Configuration"/>

</beans>
